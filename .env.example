# Database
# Local development (SQLite)
DATABASE_URL=sqlite+aiosqlite:///./codeliveos.db
# Production (PostgreSQL) - Fly.io sets this automatically
# DATABASE_URL=postgres://user:pass@host:5432/dbname

# JWT Settings
SECRET_KEY=your-super-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# GitHub OAuth (optional)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI=

# AI Integration (Anthropic Claude)
ANTHROPIC_API_KEY=
AI_MODEL=claude-sonnet-4-20250514

# OpenAI (for Whisper transcription - optional)
OPENAI_API_KEY=

# Google Places API (for address autocomplete)
GOOGLE_PLACES_API_KEY=

# Property Enrichment Database (external PostgreSQL with property data)
# ENRICHMENT_DATABASE_URL=postgresql+asyncpg://user:pass@host:5432/enrichment_db

# Resend (Email Service for sending reports)
RESEND_API_KEY=
RESEND_FROM_EMAIL=reports@yourdomain.com
